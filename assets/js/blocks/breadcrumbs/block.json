{
	"name": "woocommerce/breadcrumbs",
	"version": "1.0.0",
	"title": "Product Breadcrumbs",
	"description": "A block that allows you to add breadcrumbs to your store.",
	"category": "woocommerce",
	"keywords": [ "WooCommerce" ],
	"supports": {
		"align": true,
		"color": {
			"text": true
		},
		"typography": {
			"fontSize": true,
			"__experimentalFontFamily": true
		}
	},
	"attributes": {
		"displayStyle": {
			"type": "string",
			"default": "icon_and_text"
		}
	},
	"textdomain": "woo-gutenberg-products-block",
	"apiVersion": 2,
	"$schema": "https://schemas.wp.org/trunk/block.json"
}
